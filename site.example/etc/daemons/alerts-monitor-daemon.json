{
	"enabled": true,
	"execPath": "alerts-monitor-daemon.pl",
	"listeningPort": 12321,
	"listeningInterval": 1000,
	"messagingInterval": 60000,
	"httpingInterval": 300000,
	"textingInterval": -1,
	"monitoredDir": "[eval:TTP::alertsFileDropdir()]",
	"scanInterval" : 10000,
	"do": [
		{
			"command": "/some/bin -level <LEVEL> -emitter <EMITTER> -title <TITLE> -message <MESSAGE> -stamp <STAMP>",
			"levelRe": "NOTICE|INFO"
		}
	]
}
